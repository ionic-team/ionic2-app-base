  "scripts": {
    "webpack": "webpack --config config/webpack/webpack.config.js",
    "compile-js": "gulp bundle-js",
    "rollup": "rollup -f iife -c -o dist/rollup.js",
    "transpile-rollup": "tsc --out dist/bundle-es5.js --target es5 --allowJs dist/rollup.js",
    "minify": "uglifyjs --screw-ie8 --compress --mangle --output ./dist/bundle.min.js ./dist/bundle-es5.js",
    "gzip": "gzip -k ./dist/bundle-es5.js",
    "build": "npm run compile-js && npm run rollup && npm run transpile-rollup && npm run minify && npm run gzip"
  },
