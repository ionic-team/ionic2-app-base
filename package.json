    "core-js": "2.4.1",
    "ionic-angular": "nightly",
    "ionic-native": "1.3.20",
    "ionicons": "3.0.0",
    "rxjs": "5.0.0-beta.11",
    "zone.js": "0.6.17"
  },
  "devDependencies": {
